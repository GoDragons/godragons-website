[
  {
    "name": "web_app",
    "image": "890725220831.dkr.ecr.eu-west-2.amazonaws.com/hiit-tribe:latest",
    "essential": true,
    "portMappings": [
      {
        "containerPort": 3002,
        "hostPort": 3002
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "hiit-tribe",
        "awslogs-region": "eu-west-2",
        "awslogs-stream-prefix": "hiit-tribe"
      }
    },
    "environment": [{ "name": "ENV", "value": "test" }]
  }
]
